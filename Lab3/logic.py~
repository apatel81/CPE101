def is_even(a):
    # calculate if an integer is an even number
    # int -> true or false
    return a%2 == 0

def in_an_interval(a):
    #Determine whether a number is in one of the following intervals
    #int -> true or false
    return bool(-2<=a<9 or 22<a<42 or 12<a<=20 or 120<=a<=127)
